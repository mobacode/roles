{
    "name": "mobacode/roles",
    "description": "Powerful package for handling roles and permissions in Laravel 6",
    "version": "1.0.1",
    "keywords": [
        "roles",
        "permissions",
        "acl",
        "auth",
        "laravel",
        "mobacode",
        "mobacode"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "mobacode",
            "email": "mohamed.reda@mobacode.com"
        }
    ],
    "minimum-stability": "dev",

    "autoload": {
        "psr-4": {
            "mobacode\\Roles\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "orchestra/database": "^3.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~7.0",
        "satooshi/php-coveralls": "^2.0",
        "sebastian/environment": "^3.1"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/User.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "mobacode\\Roles\\RolesServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0"
    }
}
